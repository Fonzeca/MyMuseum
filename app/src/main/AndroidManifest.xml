        <activity android:name=".Vista.EditarInventoActivity"/>
        <activity android:name=".Vista.EditarInventorActivity"/>
        <activity android:name=".Vista.EditarPeriodoActivity"/>
        <activity android:name=".Vista.EliminarInventoActivity"/>
        <activity android:name=".Vista.EliminarInventorActivity"/>
        <activity android:name=".Vista.EliminarPeriodoActivity"/>
        <activity android:name=".Vista.ElegirEditarPersonaActivity"/>
        <activity android:name=".Vista.ElegirEliminarPersonaActivity"/>
        <activity android:name=".Vista.ElegirNuevoObjetoActivity"/>
        <activity android:name=".Vista.ElegirEliminarObjetoActivity"/>
        <activity android:name=".Vista.ArticuloInventoActivity"/>
        <activity android:name=".Vista.BuscarObjetoActivity"/>
        <activity android:name=".Vista.LectorQRActivity">
        </activity>
        <activity
            android:name=".Vista.ElegirNuevaPersonaActivity">
        </activity>
    </application>

</manifest>